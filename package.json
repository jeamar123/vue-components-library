{
  "name": "@jeamar1234/vue-components-library",
  "description": "Library for vue reusable components that can be used with projects",
  "private": false,
  "version": "0.0.1",
  "author": {
    "name": "Jeamar Libres",
    "email": "jeamar1234@gmail.com"
  },
  "repository": {
    "type": "git",
    "URL": "https://github.com/jeamar123/vue-components-library.git"
  },
  "main": "./dist/VueComponentsLibrary.cjs.js",
  "module": "./dist/VueComponentsLibrary.es.js",
  "files": [
    "dist/"
  ],
  "exports": {
    ".": {
      "import": "./dist/VueComponentsLibrary.cjs.js",
      "require": "./dist/VueComponentsLibrary.es.js."
    },
    "./style.css": "./dist/style.css"
  },
  "browser": {
    ".": "./dist/VueComponentsLibrary.es.js"
  },
  "types": "./dist/VueComponentsLibrary.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "~5.7.2",
    "vite": "^6.1.0",
    "vite-plugin-dts": "^4.5.0",
    "vue-tsc": "^2.2.0"
  }
}
